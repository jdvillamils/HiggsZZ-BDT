   "source": [
    "Now, we split simulated data to see the behavior of signal and background"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 137,
   "id": "dd9035d7",
   "metadata": {},
   "outputs": [],
   "source": [
    "sigmc=rp.read_root('RootFiles/DataTest.root', 'Signal')\n",
    "sigmc['BDTval']=rp.read_root('RootFiles/TMVAppValues.root')\n",
    "bckmc=rp.read_root('RootFiles/DataTest.root', 'Background')\n",
    "bckmc['BDTval']=rp.read_root('RootFiles/TMVAppValues.root')\n",
    "datanocut=rp.read_root('RootFiles/DataSamplesNoCuts.root')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "id": "a3b9b471",
   "metadata": {},
   "outputs": [],
   "source": [
    "signalmc=sigmc['BDTval']         \n",
    "bckgmc=bckmc['BDTval']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "9d0d1b5a",
   "metadata": {},
   "outputs": [],
   "source": [
    "sgsize=signalmc.size\n",
    "bckgsize=bckgmc.size"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "03fc1677",
   "metadata": {},
   "outputs": [],
   "source": [
    "space=30\n",
    "cuts=np.linspace(-0.8,0.6,space)\n",
    "efficiency=np.zeros(space)\n",
    "significance=np.zeros(space)\n",
    "bckgrej=np.zeros(space)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "id": "bd65d4c5",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(space):\n",
    "    cut=cuts[i]\n",
    "    efficiency[i]=signalmc[signalmc>cut].size/sgsize\n",
    "    significance[i]=signalmc[signalmc>cut].size/(bckgmc[bckgmc>cut].size)\n",
    "    bckgrej[i]=1-bckgmc[bckgmc>cut].size/bckgmc.size\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 316,
   "id": "4ebb5916",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.figure(figsize=(8,6), dpi=80)\n",
    "plt.plot(cuts,efficiency,label='Signal Efficiency',color='blue')\n",
    "plt.plot(cuts,significance,label='Signal Significance',color='red')\n",
    "plt.plot(cuts,bckgrej,label='Background Rejection',color='green')\n",
    "plt.xlabel(r'Cut value applied on BDT')\n",
    "plt.ylabel(r'Efficiency')\n",
    "plt.legend()\n",
    "plt.savefig(\"images/efficiency\")\n",
    "plt.show()"
   ]
  },
