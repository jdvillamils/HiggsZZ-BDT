   "source": [
    "signalmc=sigmc['BDTval']         \n",
    "bckgmc=bckmc"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "9d0d1b5a",
   "metadata": {},
   "outputs": [],
   "source": [
    "sgsize=signalmc.size\n",
    "bckgsize=bckgmc.size"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "03fc1677",
   "metadata": {},
   "outputs": [],
   "source": [
    "space=30\n",
    "cuts=np.linspace(-0.8,0.6,space)\n",
    "efficiency=np.zeros(space)\n",
    "significance=np.zeros(space)\n",
    "bckgrej=np.zeros(space)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "3b2678a1",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(space):\n",
    "    cut=cuts[i]\n",
    "    efficiency[i]=signalmc[signalmc>cut].size/sgsize\n",
    "    significance[i]=signalmc[signalmc>cut].size/np.sqrt(bckgmc[bckgmc>cut].size)\n",
    "    bckgrej[i]=1-bckgmc[bckgmc>cut].size/bckgmc.size\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "25efe46f",
   "metadata": {},
   "outputs": [
    {
     "data": {
